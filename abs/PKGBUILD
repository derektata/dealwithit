pkgname=dealwithit
pkgver=0.0.1
pkgrel=1
pkgdesc="A utility to print glasses in the terminal, deal with it."
arch=('x86_64')
url="https://github.com/derektata/dealwithit"
license=('GPL')
depends=('sh')
source=("https://github.com/derektata/$pkgname")
package() {
        cd "$srcdir"
	mkdir -p $pkgdir/usr/bin
	install dealwithit $pkgdir/usr/bin/$pkgname
}
md5sums=('c4d4709258344a1cb55f34a11a7dba0e')
